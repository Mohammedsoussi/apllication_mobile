#:import Factory kivy.factory.Factory
<DesiredPopup@Popup>:
    id: haja
    name: 'fma'
    title: 'result_desired is'
    auto_dismiss: False
    background_color: (20/250 ,150/250 ,50/250 ,250/255.0)
    size_hint: .95, .5
    final_bomba: final_bomba
    GridLayout:
        cols:1
        BoxLayout:
            orientation: 'vertical'
            rows:1
            Label:
                id: final_bomba
                text: str(root.final_bomba)
                font_size: 20
            Button:
                text: 'close'
                size_hint: .2, .2
                background_color: 0, 1, 0, 1
                on_press:
                    root.dismiss()

<EcolesPopup@Popup>:
    id: ecoles
    name: 'ecoles'
    title: 'possible to '
    auto_dismiss: False
    background_color: (20/250 ,150/250 ,50/250 ,250/255.0)
    name_ecoles: name_ecoles
    size_hint: .95, .5
    GridLayout:
        cols: 1
        BoxLayout:
            orientation: 'vertical'
            rows:1
            Label:
                id: name_ecoles
                text: str(root.name_ecoles)
                font_size: 20
                size: self.size
            Button:
                text: 'close'
                size_hint: .2, .2
                pos_hint: {'x': .01 ,'y': .2}
                background_color: 0, 1, 0, 1
                on_press:
                    root.dismiss() 
<QuestionPopup@Popup>:
    id: quiz
    name: 'q'
    padding: 60,40
    auto_dismiss: False
    size_hint: .7, .4
    title: 'ecole seupeurieur'
    GridLayout:
        cols:1
        BoxLayout:
            rows:1
            Label:
                id: question1
                text: '                     you want to show ecole seupeurieur'
                pos_hint: {'x': .1, 'y': .35}
            Button:
                id: accepted
                text: 'yes'
                bold:True
                size: self.size
                size_hint: .3, .3
                on_press:
                    root.dismiss()
                    app.root.current= 're_input'
            Button:
                id: refused
                text: 'None'
                bold: True
                size: self.size
                size_hint: .3, .3
                on_press:
                    root.dismiss()
<ResultPopup@Popup>:
    id: final
    name: 'fin'
    final: final 
    auto_dismiss: False
    size_hint: .6, .4
    padding: 60,40
    title: 'your result is '
    GridLayout:
        cols:1
        BoxLayout:
            rows:1
            orientation: 'vertical'
            Label:
                id: texture
                height:150
                text: str(root.final)
                pos_hint: {'x': .01, 'y': .4}
                font_size:25
            Button:
                id: 'close me'
                size_hint: .3, .2
                height: 40
                text: 'close'
                on_press:
                    Factory.QuestionPopup().open()
                    root.dismiss()
